{
  "name": "faq_server",
  "version": "1.0.0",
  "dependencies": {
    "algoliasearch": "^3.35.1",
    "express": "^4.17.3",
    "express-force-https": "^1.0.0",
    "express-jwt": "^6.0.0",
    "form-data": "^2.5.1",
    "graphql-yoga": "^1.18.3",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^1.6.0",
    "lodash": "^4.17.21",
    "mjml": "^4.6.3",
    "node-fetch": "^2.6.7",
    "path": "^0.12.7",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16",
    "prisma-multi-tenant": "^1.0.3",
    "showdown": "^1.9.1",
    "showdown-xss-filter": "^0.2.0",
    "slugify": "^1.3.6"
  },
  "scripts": {
    "start": "dotenv -e .env.local node src/index.js",
    "dev": "dotenv -e .env.local nodemon src/index.js",
    "deploy": "cd scripts/prisma_deploy_all && dotenv -e ../../.env.local node index.js",
    "new_service": "cd scripts/prisma_new_service && dotenv -e ../../.env.local node index.js",
    "generate_schema": "prisma generate -e .env.local",
    "lint": "eslint src --ext js",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier src/**/*",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "local_containers": "cd prisma/ && dotenv -e ../.env.local docker-compose up",
    "token": "dotenv -e .env.local bash scripts/prisma_token/token.sh"
  },
  "lint-staged": {
    "./src/**/*.js": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "dotenv-cli": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^3.1.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "prisma-yml": "^1.34.10"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "singleQuote": true
  }
}
